<nav class="site-nav">
    <div class="site-nav-left">
        {{!-- {{^is "home"}} --}}
            {{#if @blog.logo}}
                <a class="site-nav-logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@site.url}}">{{@site.title}}</a>
            {{/if}}
        {{!-- {{/is}}  --}}
        {{#if @blog.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links">
            
            {{#if @blog.facebook}}
            <a class="social-link social-link-ig" href="https://instagram.com/voileinc" target="">{{> "icons/instagram"}}</a>
                <a class="social-link social-link-fb" href="{{facebook_url @blog.facebook}}" title="Facebook" target="_blank" rel="noopener">{{> "icons/facebook"}}</a>
            {{/if}}
            {{#if @blog.twitter}}
                <a class="social-link social-link-tw" href="{{twitter_url @blog.twitter}}" title="Twitter" target="_blank" rel="noopener">{{> "icons/twitter"}}</a>
            {{/if}}
        </div>
        {{#if @labs.subscribers}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{else}}
            <a class="rss-button" href="https://feedly.com/i/subscription/feed/{{@site.url}}/rss/" title="RSS" target="_blank" rel="noopener">{{> "icons/rss"}}</a>
        {{/if}}
        {{#if @labs.members}}
            <a class="signin-button" href="#signin" data-members-signin>Sign In</a>
            <a class="signout-button" href="#signout" data-members-signout>Sign Out</a>
        {{/if}}
        <a class="btn btn-primary" href="#" role="button">Sign Up Early</a>
    </div>
</nav>
